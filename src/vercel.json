{
  "version": 2,
  "builds": [
    {
      "src": "src/index.html",
      "use": "@vercel/static-build",
      "config": {}
    }
  ],
  "routes": [
    {
      "src": "/.well-known/webfinger.txt",
      "dest": "/.well-known/webfinger.txt"
    }
  ],
  "redirects": [
    {
      "source": "/.well-known",
      "destination": "/.well-known/webfinger.txt",
      "permanent": true
    }
  ]
}
